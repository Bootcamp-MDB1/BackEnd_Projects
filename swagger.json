      "post": {
        "tags": [
          "Resto Profiles"
        ],
        "summary": "Create new restoran profile in list",
        "description": "Create new restoran profile using Json payload",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "new restoran profile",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "cafe 2"
                },
                "address": {
                  "type": "string",
                  "example": "location"
                },
                "category": {
                  "type": "string",
                  "example": "seafood"
                }
              }
            }
          }
        ],
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "successful create new restoran profile"
          },
          "422": {
            "description": "failed to process"
          }
        }
      }
    },
